<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <table border="1">
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Created At</th>
      </tr>
      <% for(var i=0; i < employee_list.length; i++) { %>
        <tr>
          <td><%= employee_list[i].first_name %></td>
          <td><%= employee_list[i].last_name %></td>
          <td><%= employee_list[i].email %></td>
          <td><%= employee_list[i].created_at %></td>
        </tr>
      <% } %>
  </table>

  </body>
</html>
